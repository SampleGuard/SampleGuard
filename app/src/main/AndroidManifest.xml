<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sampleguard">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:launchMode="singleTop"
        android:theme="@style/AppTheme"
        android:background="@color/blue">
        <activity android:name="com.example.sampleguard.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sampleguard.ProfilingActivity"
            android:label="@string/app_name"></activity>
        <activity android:name="com.example.sampleguard.GraphActivity"
            android:label="@string/app_name"></activity>
        <service android:name="com.example.sampleguard.ExampleService" />
    </application>



</manifest>